@{
    ViewData["Title"] = "Usuários";
}

<link rel="stylesheet" href="~/css/Principal.css" asp-append-version="true" />

@{
    <p>Usuários</p>
    if (ViewBag.listaUsuarios != null)
    {
        foreach (var item in ViewBag.listaUsuarios)
        {
            <br /><br />
            <div class="row" id="resultado">
                <div class="col-25">

                    <h4>@item.nome</h4>
                    <p>Id: @item.id</p>
                    <p>Email: @item.email</p>
                    <p>Telefone: @item.telefone</p>
                    <p>Sexo: @item.sexo</p>
                    <p>Data de nascimento: @item.dataNascimento</p>

                </div>
            </div>
        }
    }
}
